<% include header %>
<div class="container bs-wcy_citybox">
    <div class="bs-wcy_govnoticetitle">政府公告</div>
    <table class="table table-striped bs-wcy_govnoticeTab">
        <thead>
        <tr>
            <th width="75%" height="35">标题</th>
            <th width="25%">发布时间</th>
        </tr>
        </thead>
        <tbody>
        <% newses.forEach(function(news){ %>
        <tr>
            <td height="55"><a href="noticeDetail/<%= news._id %>"><%= news.title %></a></td>
            <td><%= new Date(news.createTime).Format('yyyy年MM月dd日hh:mm:ss') %></td>
        </tr>
        <% }); %>
        </tbody>
    </table>
    <div class="bs-wcy_govnoticePage">
        <div class="quotes"><span class="current">1</span><a href="#?page=2">2</a><a href="#?page=next">下一页</a><a
                    href="#?page=end">尾页</a></div>
    </div>
</div>
<% include footer %>
<script src="/js/jquery.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
